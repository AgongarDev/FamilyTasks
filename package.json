{
  "name": "com.capencara.portfolio",
  "version": "1.0.0",
  "description": "My first portfolio made with React and several backend technologies",
  "main": "index.js",
  "scripts": {
    "test": "-t",
    "start": "parcel src/index.html",
    "lint": "eslint --fix src/**/*.{js, jsx}",
    "format": "prettier --write \"*.{js, jsx, json, css}\""
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgongarDev/portfolio.git"
  },
  "keywords": [
    "portfolio",
    "react",
    "html5",
    "javascript",
    "professional",
    "frontend",
    "backend"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js, jsx}": [
      "npm run lint"
    ],
    "src/**/*.{js, jsx, json, css, scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Antonio Gonz√°lez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgongarDev/portfolio/issues"
  },
  "homepage": "https://github.com/AgongarDev/portfolio#readme",
  "dependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react": "^7.30.1",
    "parcel-bundler": "^1.3.1"
  }
}
